{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug philo",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/philo/philo",
      "args": [
        "5",
        "800",
        "200",
        "200"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/philo",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build philo"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/alpayet/projects/Philosophers/philo/src",
      "program": "/home/alpayet/projects/Philosophers/philo/src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}